<div class="row-fluid" id="projectIndexUl">
<% projects.each(function(proj) { %>
	<div class="col-xs-3">
		<div class="splashContainer projectLi">
		<a href="#/project/<%= proj.id %>">
			<p class="splashTitle"><%= proj.escape('name') %></p>
		<% proj.get('splash').each(function(photo){ %>
			<img class="splashPhoto" src="<%= photo.get('img_url') %>">
		<% }) %>
		</a>
		</div>
	</div>

<% }) %>
</div>

